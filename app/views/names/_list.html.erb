<div id="filterrific_results">
  <div class="results_info">
    <span id="results_number"><%= @number_of_results.to_s %></span> emra plotësojnë kushtet e parashtruara për kërkim:
  </div>
  <table class="table table-striped tabbody">
      <tr class="warning">
      <th>Emri</th>
      <th>Gjinia</th>
      <th>Kuptimi</th>
      </tr>
 
      <% names.each do |n| %>
      <tr>
         <td><strong><%= n.name %></strong></td>
         <td><%= n.name_gender.name %></td> 
	       <td><%= n.meaning %></td> 
      </tr>
  <% end %>
  </table>
</div>

